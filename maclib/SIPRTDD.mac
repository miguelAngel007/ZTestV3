               &SPGNM=,          SYSOUT PROGRAM NAME                   X00001100
               &SFMNO=,          SYSOUT FORM NUMBER                    X00001200
               &OUTLM=,          OUTPUT LIMIT                          X00001300
               &OUTLIM=,         OUTPUT LIMIT                          X00001400
               &COPYS=,          SYSOUT COPIES                         X00001500
               &COPIES=,         SYSOUT COPIES                         X00001600
               &FCBIM=,          FCB IMAGE-ID                          X00001700
               &FCB=,            FCB IMAGE-ID                          X00001800
               &FCBAV=,          FCB FORM ALIGNMENT,IMAGE VERIFY       X00001900
               &UCS=,            UNIVERSAL CHARACTER SET               X00002000
               &UFOLD=,          UCS FOLD MODE                         X00002100
               &FOLD=,           UCS FOLD MODE                         X00002200
               &UVRFY=,          UCS VERIFY CHARACTER SET              X00002300
