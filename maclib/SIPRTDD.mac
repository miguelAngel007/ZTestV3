&DALLBL(&DALNO) SETC 'SYSOUDA'                                          00014700
&DALKEY(&DALNO) SETC 'AL2(DALSYSOU)'                                    00014800
         AIF   ('&WORK' NE '-').F020100                                 00014900
.*       A DASH SPECIFIED FOR SYSOUT CLASS MEANS THE DEFAULT CLASS      00015000
&DALOCC(&DALNO) SETC 'AL2(0)'                                           00015100
&DALLEN(&DALNO) SETC ''                                                 00015200
&DALDTA(&DALNO) SETC ''                                                 00015300
         AGO   .F020END                                                 00015400
.F020100 ANOP                                                           00015500
&DALOCC(&DALNO) SETC 'AL2(1)'                                           00015600
&DALLEN(&DALNO) SETC 'AL2(1)'                                           00015700
&DALDTA(&DALNO) SETC 'CL1''&WORK'''                                     00015800
.F020END ANOP                                                           00015900
