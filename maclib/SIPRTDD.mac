.F010020 ANOP                                                           00010900
         AIF   (&DDNM EQ 1).F010030                                     00011000
         MNOTE '&H DDNAM/DDNAME OMITTED, SIPRTDD USED AS DEFAULT'       00011100
.F010030 ANOP                                                           00011200
&DALNO   SETA  &DALNO+1                                                 00011300
         AIF   (&DALNO GT &MAXENT).Z010010                              00011400
&DALLBL(&DALNO) SETC 'DDNAMDA'                                          00011500
&DALKEY(&DALNO) SETC 'AL2(DALDDNAM)'                                    00011600
&DALOCC(&DALNO) SETC 'AL2(1)'                                           00011700
&DALLEN(&DALNO) SETC 'AL2(8)'                                           00011800
&DALDTA(&DALNO) SETC 'CL8''&WORK'''                                     00011900
.F010END ANOP                                                           00012000
