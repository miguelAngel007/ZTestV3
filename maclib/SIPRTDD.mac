               &BURST=,          BURSTER-TRIMMER-STACKER               X00002900
               &CHARS=,          CHAR ARRANGEMENT TABLE                X00003000
               &COPYG=,          COPY GROUP VALUES                     X00003100
               &FFORM=,          FLASH FORMS OVERLAY                   X00003200
               &FCNT=,           FLASH FORMS OVERLAY COUNT             X00003300
               &MMOD=,           COPY MODIFICATION MODULE              X00003400
               &MODIFY=,         COPY MODIFICATION MODULE              X00003500
               &MTRC=,           TABLE REFERENCE CHARACTER             X00003600
               &TRC=,            TABLE REFERENCE CHARACTER             X00003700
               &OUTPT=,          OUTPUT REFERENCE                      X00003800
               &OUTPUT=,         OUTPUT REFERENCE                      X00003900
               &DEST=,           SYSOUT REMOTE NODE ID AND USER ID     X00004000
               &TESTRUN=         TESTRUN SPECIFICATION                  00004100
.*                                                                      00004200
.*       MOST OF THE VARIABLE NAMES ABOVE ARE TAKEN FROM THE LIST OF    00004300
.*       DYNAMIC ALLOCATION KEYS LISTED IN THE IEFZB4D2 MACRO WITH THE  00004400
.*       "DAL" PREFIX OMITTED FOR COMPATABILITY WITH ASSEMBLER F.       00004500
.*       HOWEVER, SOME OF THE VARIABLE NAMES HAVE BEEN DUPLICATED SO    00004600
.*       THAT THE NORMAL DCB/DD VARIABLE NAMES CAN BE USED.  IF BOTH    00004700
.*       VERSIONS HAPPEN TO BE CODED, THE NAME DERIVED FROM THE         00004800
.*       IEFZB4D2 MACRO WILL TAKE PRECEDENCE.                           00004900
.*                                                                      00005000
.*                                                                      00005100
         GBLB  &SIFCTB,&SIPRTDD,&DALFLAG                                00005200
         GBLC  &CSECTNM                                                 00005300
         LCLA  &DALNO,&CNT,&MAXENT,&X1,&X2                              00005400
         LCLA  &RECFMA,&WORKA                                           00005500
         LCLB  &ERR,&FCBSPEC,&UCSSPEC,&OUTSPEC,&DDNM                    00005600
         LCLC  &DALLBL(50),&DALKEY(50),&DALOCC(50)                      00005700
         LCLC  &DALLEN(50),&DALDTA(50)                                  00005800
         LCLC  &H,&HVER,&HMOD,&HDATE                                    00005900
         LCLC  &WORK,&WORKCH,&WLABEL,&LBL,&SUF                          00006000
.*                                                                      00006100
.*       ----------------                                               00006200
.*       SET ANY DEFAULTS                                               00006300
.*       ----------------                                               00006400
&H       SETC  '* SIPRTDD -'       MACRO NAME FOR MNOTE LINES           00006500
&HVER    SETC  '01'                MACRO VERSION NUMBER                 00006600
&HMOD    SETC  '01'                MACRO MODIFICATION NUMBER            00006700
&HDATE   SETC  '12/15/91'          MACRO AS OF DATE                     00006800
         MNOTE '&H SYSTEMATICS  - VER &HVER MOD &HMOD - &HDATE'         00006900
&MAXENT  SETA  50                  MAXIMUM ENTRIES IN TABLE             00007000
&LBL     SETC  'PROD'                                                   00007100
&SUF     SETC  'P'                                                      00007200
         AIF   (T'&TESTRUN NE 'O').E010010                              00007300
         AIF   (NOT &DALFLAG).E010030                                   00007400
         MNOTE 8,'&H CANNOT SPECIFY TWO DEFAULT SETS OF DD PARAMETERS'  00007500
         MEXIT                                                          00007600
.E010010 ANOP                                                           00007700
         AIF   ('&TESTRUN' EQ 'YES').E010020                            00007800
         MNOTE '&H TESTRUN MUST BE YES IF SPECIFIED, YES ASSUMED'       00007900
.E010020 ANOP                                                           00008000
&LBL     SETC  'TEST'                                                   00008100
&SUF     SETC  'T'                                                      00008200
.E010030 ANOP                                                           00008300
.*                                                                      00008400
.*       -----------                                                    00008500
.*       CHECK DDNAM                                                    00008600
.*       -----------                                                    00008700
.*                                                                      00008800
.F010BEG ANOP                                                           00008900
&WORK    SETC  'SIPRTDD'                                                00009000
         AIF   (T'&DDNAM EQ 'O').F010010                                00009100
&WORK    SETC  '&DDNAM'                                                 00009200
&DDNM    SETB  1                                                        00009300
         AIF   (K'&DDNAM LE 8).F010020                                  00009400
         MNOTE 8,'&H DDNAM MUST BE 1 TO 8 CHARACTER(S) IN LENGTH'       00009500
&ERR     SETB  (1)                                                      00009600
         AGO   .F010END                                                 00009700
.*       ------------                                                   00009800
.*       CHECK DDNAME                                                   00009900
.*       ------------                                                   00010000
.F01010  ANOP                                                           00010100
         AIF   (T'&DDNAME EQ 'O').F010020                               00010200
&WORK    SETC  '&DDNAME'                                                00010300
&DDNM    SETB  1                                                        00010400
