         GBLA  &LVCTL3,&HVCTL3,&LHDRLIN                                 00004100
         GBLA  &LCUST,&LRESP,&LRESP2                          2120563   00004200
         GBLA  &LVCUST,&HVCUST                                          00004300
         GBLA  &SEQCNT,&MAXSEQ                                          00004400
         GBLA  &NOKEYS                                                  00004500
         GBLB  &GIAPPL,&ERR,&GIVER,&SEQTBL,&SRT,&FMS,&SIFCTB            00004600
         GBLB  &FIRSTSW,&INITIAL,&FINAL                                 00004700
         GBLC  &H,&MVER,&MMOD,&MDATE,&HAPPL,&HVER                       00004800
         GBLC  &T(1000),&V(1000)                                        00004900
         GBLC  &SEQ1(32),&SEQ2(32)                                      00005000
         GBLC  &LVRESP,&HVRESP                                          00005100
         GBLC  &KEYWD(60)                                               00005200
         GBLC  &QUOTE,&CSECTNM                                          00005300
         LCLA  &WORK,&G,&J                                              00005400
         LCLA  &X1,&X2,&X3,&X4,&X5,&Y1,&Y2,&Y3                          00005500
         LCLB  &INIT,&REQUIRE,&END,&LIBANK,&LIFORM                      00005600
         LCLB  &LIRPTNM,&LIDDNM,&LICTL2,&LICTL3                         00005700
         LCLB  &LICUST,&LIRESP                                          00005800
         LCLC  &HBANK,&HCTL2,&HCTL3,&HRPTNM,&WE                         00005900
         LCLC  &HDDNM,&HDDNM1,&HDDNM2,&HDDNM3                           00006000
         LCLC  &HHDRLIN,&S(16)                                          00006100
         LCLC  &HCUST,&HRESP                                            00006200
         LCLC  &RESPWK,&RESPCH                                          00006300
         LCLC  &DDNMPRT(10),&WORKC,&WORKC1,&WORKC2,&WORKC3,&WORKC4      00006400
